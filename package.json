{
  "name": "zenflow-backend",
  "version": "1.0.0",
  "description": "ZenFlow Backend API - Node.js/Express/TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "dev:nodemon": "nodemon --watch src --ext ts,json --exec 'tsx src/index.ts' --signal SIGTERM",
    "dev:debug": "NODE_OPTIONS='--inspect' tsx watch --clear-screen=false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --ci --maxWorkers=2",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/__tests__/e2e-*.test.ts' --testTimeout=30000",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/__tests__/**/*.test.ts' --testPathIgnorePatterns='e2e-*.test.ts'",
    "seed:dev": "tsx src/scripts/seedDev.ts",
    "backup:setup": "tsx src/scripts/setup-backup-cron.ts",
    "monitoring:verify": "./scripts/verify-monitoring.sh",
    "migrate:courses": "tsx scripts/migrate-courses.ts"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^10.31.0",
    "@sentry/profiling-node": "^10.31.0",
    "@supabase/supabase-js": "^2.87.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/exceljs": "^0.5.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/web-push": "^3.6.4",
    "@upstash/redis": "^1.35.8",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "prom-client": "^15.1.3",
    "resend": "^6.6.0",
    "stripe": "^20.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.15.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.2.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
